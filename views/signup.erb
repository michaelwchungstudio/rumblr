<div class='sign-up-wrapper'>
  <h2 class='page-title'>Sign Up</h2>

  <form id='sign-up-form' method='post' action='/signup'>
    <h3 class='sign-up-title'>Username</h3>
    <input class='sign-up-input' type="text" name="username">
    <h3 class='sign-up-title'>Password</h3>
    <input class='sign-up-input' type="text" name="password">
    <h3 class='sign-up-title'>First Name</h3>
    <input class='sign-up-input' type="text" name="firstname">
    <h3 class='sign-up-title'>Last Name</h3>
    <input class='sign-up-input' type="text" name="lastname">
    <h3 class='sign-up-title'>Birthday</h3>
    <input class='sign-up-input' type="text" name="birthday">
    <h3 class='sign-up-title'>Email</h3>
    <input class='sign-up-input' type="text" name="email">
    <button class='sub-button' type="submit">SUBMIT</button>
  </form>
</div>
